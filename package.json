    "rfdc",
    "safe-buffer",
    "safer-buffer",
    "streamroller",
    "util-deprecate",
    "uuid",
    "xregexp",
    "request",
    "extend",
    "tough-cookie",
    "json-stringify-safe",
    "hawk",
    "boom",
    "hoek",
    "sntp",
    "cryptiles",
    "aws-sign2",
    "aws4",
    "http-signature",
    "assert-plus",
